class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n=nums.size();
        int p=0;
        vector<int>s(nums);
        for(int i=0;i<n;i++)
        {
            nums[(i+k)%n]=s[i];
            
        }
        /*for(int i=0;i<n;i++)
        {
            cout<<nums[i]<<" ";
        }*/
    }
};
